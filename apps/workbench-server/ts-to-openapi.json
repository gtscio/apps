{
	"title": "GTSC Workbench Server",
	"version": "1.0.0",
	"description": "REST API for Global Trade and Supply Chain - API Server.",
	"licenseName": "Apache 2.0 License",
	"licenseUrl": "https://opensource.org/licenses/Apache-2.0",
	"servers": ["https://workbench-api.gtsc.io"],
	"restRoutes": [
		{
			"package": "@gtsc/api-service"
		},
		{
			"package": "@gtsc/api-auth-entity-storage-service"
		},
		{
			"package": "@gtsc/identity-service"
		},
		{
			"package": "@gtsc/blob-storage-service"
		},
		{
			"package": "@gtsc/telemetry-service"
		},
		{
			"package": "@gtsc/logging-service"
		},
		{
			"package": "@gtsc/nft-service"
		},
		{
			"package": "@gtsc/attestation-service"
		},
		{
			"package": "@gtsc/auditable-item-graph-service",
			"entryPoints": [
				{
					"name": "auditable-item-graph",
					"baseRoutePath": "aig"
				}
			]
		},
		{
			"package": "@gtsc/auditable-item-stream-service",
			"entryPoints": [
				{
					"name": "auditable-item-stream",
					"baseRoutePath": "ais"
				}
			]
		}
	],
	"authMethods": ["jwtBearer"],
	"externalReferences": {
		"IJsonLd(.*?)": "https://schema.gtsc.io/json-ld/JsonLd$1",
		"I?AuditableItemGraph(.*?)(?<!Request|Response)": "https://schema.gtsc.io/aig/AuditableItemGraph$1",
		"I?AuditableItemStream(.*?)(?<!Request|Response)": "https://schema.gtsc.io/ais/AuditableItemStream$1"
	}
}
