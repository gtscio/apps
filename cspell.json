{
	"language": "en-GB",
	"ignorePaths": [
		"**/node_modules/**",
		"LICENSE",
		"**/coverage/**",
		"**/dist/**",
		"**/docs/reference/**",
		"**/docs/open-api/**",
		"**/tests/**",
		"*.yaml",
		"*.svg",
		"package.json",
		"rollup.config.mjs",
		"vitest.config.*",
		"tsconfig.json",
		"typedoc.json",
		".eslintrc.cjs",
		"**/wordlists/**"
	],
	"words": [
		"bech",
		"certbot",
		"color",
		"Fastify",
		"faucet",
		"flowbite",
		"GTSC",
		"ipfs",
		"nameof",
		"openapi",
		"scylladb",
		"secp",
		"Stiftung",
		"Swatchbook",
		"tailwindcss",
		"unrevoke",
		"Unrevoking",
		"whereis",
		"WORKDIR"
	],
	"ignoreRegExpList": [
		"/from\\s+(['\"]).*\\1/",
		"/eslint-disable.*/",
		"/^```shell[\\s\\S]*?```$/gm"
	]
}
